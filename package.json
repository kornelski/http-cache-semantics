    "devDependencies": {
        "@types/jest": "^29.5.3",
        "jest": "^29.6.2",
        "miniflare": "^3.20240129.1",
        "jest-environment-miniflare": "^2.14.2",
        "ts-jest": "^29.1.1",
        "tsup": "^8.0.1",
        "typescript": "latest"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
